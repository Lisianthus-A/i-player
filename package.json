{
  "name": "i-player",
  "version": "1.0.0",
  "description": "A pure local music player",
  "main": "main.js",
  "scripts": {
    "electron": "cross-env NODE_ENV=development electron .",
    "front": "vite",
    "build": "tsc && vite build && electron-builder"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lisianthus-a/i-player.git"
  },
  "keywords": [
    "music",
    "player",
    "react",
    "electron"
  ],
  "author": "lisianthus-a",
  "license": "MIT",
  "build": {
    "appId": "com.lisianthus.i.player",
    "productName": "I Player",
    "win": {
      "target": [
        "nsis"
      ]
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowToChangeInstallationDirectory": true
    }
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.1",
    "butterchurn": "^2.6.7",
    "butterchurn-presets": "^2.4.7",
    "classnames": "^2.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.8"
  },
  "devDependencies": {
    "@types/node": "^17.0.23",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@vitejs/plugin-react": "^1.2.0",
    "cross-env": "^7.0.3",
    "electron": "^17.1.2",
    "electron-builder": "^23.0.3",
    "sass": "^1.50.0",
    "typescript": "^4.6.3",
    "vite": "^2.8.6"
  }
}